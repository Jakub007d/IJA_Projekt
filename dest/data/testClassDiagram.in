@startuml

class Person {
+ String name
- int age
+ String idNumber
+ op int getName <-String name int kôkôt
}
class Post_Office {
+ String name
+ String openingHours
+ op String getOpeningHours .
}
class Package {
+ String trackId
+ Date dateOfSend
+ op String getTackId .
}
relation Person 1 1 Post_Office isIn
relation Person many 1 Package have
relation Post_Office many 1 Package have

@enduml